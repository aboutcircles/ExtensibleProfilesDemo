{
    "@context": {
    "@version": 1.1,
        "@protected": true,

        "circles": "https://aboutcircles.com/vocab#",
        "schema": "https://schema.org/",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        
        "cidv0": "circles:cidv0",
        "hex": "circles:hex",
        "ethAddress": "circles:ethAddress",
        "unixSeconds": "circles:unixSeconds",

        "cid":         { "@id": "circles:cid",         "@type": "cidv0" },
        "chainId":     { "@id": "circles:chainId",     "@type": "xsd:integer" },
        "signerAddress": { "@id": "circles:signerAddress", "@type": "ethAddress" },
        "signedAt":    { "@id": "circles:signedAt",    "@type": "unixSeconds" },
        "nonce":       { "@id": "circles:nonce",       "@type": "hex" },
        "signature":   { "@id": "circles:signature",   "@type": "hex" },
    
        "encrypted":   { "@id": "circles:encrypted",   "@type": "xsd:boolean" },
        "encryptionAlgorithm":       { "@id": "circles:encryptionAlgorithm" },
        "encryptionKeyFingerprint":  { "@id": "circles:encryptionKeyFingerprint", "@type": "hex" },
    
        "name":        { "@id": "schema:name" },
        "description": { "@id": "schema:description" },
        "imageUrl":    { "@id": "schema:image", "@type": "@id" },
        "previewImageUrl": { "@id": "circles:previewImage", "@type": "@id" }
    }
}
